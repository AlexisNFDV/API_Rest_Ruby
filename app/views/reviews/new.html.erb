<h1>Nouveau commentaire for <%=@computer.name%></h1>
<%= form_for([@computer, @review]) do |f|%>
    <div class="field">
        <%=f.label :title%>
        <%=f.text_field :title%>
    </div>
    <div class="field">
        <%=f.label :content%>
        <%=f.text_field :content%>
    </div>
    <div class="actions">
        <%=f.submit%>
    </div>
<%end%>

<%= link_to 'Back', computers_path %>