

<h1>Computers show with_filter</h1>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Processor</th>
      <th>Storage</th>
      <th>Ram</th>
      <th>Size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @computer.each  do |computer| %>
      <tr>
        <td><%= computer.name %></td>
        <td><%= computer.processor %></td>
        <td><%= computer.storage %></td>
        <td><%= computer.ram %></td>
        <td><%= computer.size %></td>
        <td><%= link_to 'Show', computer %></td>
        <td><%= link_to 'Edit', edit_computer_path(computer) %></td>
        <td><%= link_to 'Destroy', computer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Search', '/with_filter' %>